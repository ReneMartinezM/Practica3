#Hasta aqui tenemos ua lista donde solo viene los numeros del numerador y pegado el /
cad =" "
cadena = Operacion.converToString(dcilist)
lista = re.findall(patrondeci,cadena)
print(lista)
#Aqui igual, pero esto se depuara para tner iuna lista sin el operador / 
nuevaCad = Operacion.converToString(lista)
cadenaNumeradores = re.sub('/','',nuevaCad)
print(cadenaNumeradores)
listaNueva = cadenaNumeradores.split(sep=' ')
print(listaNueva)
for i in range(0,len(listaNueva)):
  if(listaNueva[i]!=''):
    num = Cal.division(listaNueva[i],listaNueva[i+1])
    cad = cad +" "+ format(num)
    #cad = cad +" "+ format(1.0)
print(cad) 
